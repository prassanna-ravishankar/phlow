{
  "name": "phlow-auth",
  "version": "0.1.0",
  "description": "JWT authentication middleware for AI agents with Supabase integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "echo 'No tests configured yet'",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "a2a-js": "^0.2.0",
    "@supabase/supabase-js": "^2.43.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "phlow",
    "authentication",
    "agent",
    "a2a",
    "supabase",
    "middleware",
    "jwt"
  ],
  "author": "Prassanna Ravishankar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prassanna-ravishankar/phlow.git",
    "directory": "packages/phlow-auth-js"
  },
  "bugs": {
    "url": "https://github.com/prassanna-ravishankar/phlow/issues"
  },
  "homepage": "https://prassanna.io/phlow/"
}
