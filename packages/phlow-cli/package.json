{
  "name": "phlow-cli",
  "version": "0.1.0",
  "description": "CLI tools for Phlow - Agent-to-Agent authentication framework",
  "bin": {
    "phlow": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist node_modules .turbo",
    "postinstall": "node dist/postinstall.js || true"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.19",
    "ora": "^8.0.1",
    "node-forge": "^1.3.1",
    "@supabase/supabase-js": "^2.43.1",
    "dotenv": "^17.0.1",
    "fs-extra": "^11.2.0",
    "phlow-auth": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/node-forge": "^1.3.11",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "phlow",
    "cli",
    "authentication",
    "agent",
    "a2a",
    "supabase"
  ],
  "author": "Phlow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phlowai/phlow.git",
    "directory": "packages/phlow-cli"
  },
  "bugs": {
    "url": "https://github.com/phlowai/phlow/issues"
  },
  "homepage": "https://github.com/phlowai/phlow#readme"
}