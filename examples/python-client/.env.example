# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Agent Configuration
AGENT_ID=python-agent-001
AGENT_NAME=Python Agent Example
AGENT_DESCRIPTION=A Python-based Phlow agent using FastAPI
AGENT_PERMISSIONS=read:data,write:data

# Agent RSA Keys (generate via phlow CLI or manually)
AGENT_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nYOUR_PUBLIC_KEY_HERE\n-----END PUBLIC KEY-----"
AGENT_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END RSA PRIVATE KEY-----"

# Server Configuration
PORT=8000
ENVIRONMENT=development

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000